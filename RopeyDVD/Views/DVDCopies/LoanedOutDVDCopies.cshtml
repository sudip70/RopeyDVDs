@*@model IEnumerable<RopeyDVD.Models.Members>*@
@{
    ViewData["Title"] = "LoanedOutDVDCopies";
}
<h1>DVD loaned</h1>
@if(ViewBag.NoData != null)
{
    <h3>@ViewBag.NoData</h3>
}
else
{
    <p>Total loans taken at @ViewBag.SelectedDate : @ViewBag.TotalLoans</p>
    <table class="table">
    <thead>
        <tr>
            <th>Loan Number</th>
            <th>DVD Title</th>
            <th>Copy Number</th>
            <th>Member</th>
            <th>Date Out</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Loan</td>
                <td>@item.DVDTitle</td>
                <td>@item.Copy</td>
                <td>@item.Member</td>
                <td>@item.DateOut</td>
            </tr>
        }
    </tbody>
</table>

}