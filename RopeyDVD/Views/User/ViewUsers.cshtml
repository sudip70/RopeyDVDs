@{
    ViewData["Title"] = "ViewUserDetails";
}

<h1>ViewUserDetails</h1>
<a class="btn btn-success" asp-action="CreateUser">Create New</a>
<table class="table">
    <thead>
        <tr>
            <th>UserName</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>