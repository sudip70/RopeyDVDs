@{
    Layout = "_LayoutAdmin";
    ViewData["Title"] = "Unpopular DVDs";
}
<h1>Unpopular DVDs</h1>
<p class="txt-secondary">List of DVD titles in the store which has not been loaned in last 31 days</p>

<table class="table">
<thead>
    <tr>
        <th>Copy Number</th>
        <th>DVD Title</th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CopyNumber</td>
            <td>@item.Title</td>
        </tr>
    }
</tbody>
</table>
